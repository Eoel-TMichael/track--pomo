{
    "expo": {
        "name": "track_pomo",
        "slug": "track_pomo",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "plugins": [
            [
                "expo-notifications",
                {
                    "icon": "./assets/icon.png",
                    "color": "#ffffff",
                    "sounds": ["./assets/music/bell.mp3"]
                }
            ]
        ],
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "requireFullScreen": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "permissions": ["MOTION"],
            "package": "com.joelmeles.track_pomo"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
